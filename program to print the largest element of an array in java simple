public class Main {
    public static void main(String[] args) {
        int[] arr = {10, 2, 45, 30, 7};
        int max = arr[0];

        for (int i = 1; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
            }
        }

        System.out.println("Largest element in the array is: " + max);
    }
}
